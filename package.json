{
  "name": "youtubeapi-edge",
  "version": "0.0.12",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/myzkey/youtubeapi-edge"
  },
  "description": "A YouTube API client for Edge environments",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && tsc --project tsconfig.build.json && tsc-alias",
    "prepare": "pnpm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "lint": "eslint src/",
    "lint:fix": "prettier --write 'src/**/*' &&  eslint --fix src/"
  },
  "keywords": [
    "YouTube",
    "API",
    "Edge"
  ],
  "author": "myzkey",
  "license": "MIT",
  "packageManager": "pnpm@9.12.3",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.9.4",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "happy-dom": "^15.11.7",
    "prettier": "^3.4.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}